<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eageawning2.app.eage_awning2">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:fullBackupContent="false"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"

        >
        <activity android:name="com.eageawning2.app.eage_awning2.SplashActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme"
            android:windowSoftInputMode="adjustResize|stateHidden"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.eageawning2.app.eage_awning2.MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme"
            android:windowSoftInputMode="adjustResize|stateHidden"/>
        <activity android:name="com.eageawning2.app.eage_awning2.ResultActivity"
            android:screenOrientation="portrait"
            android:theme="@style/resultTheme">

        </activity>
        <activity android:name="com.eageawning2.app.eage_awning2.popupActivity"
            android:theme="@style/popupTheme"
            android:screenOrientation="portrait"/>
    </application>

</manifest>